<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>todo</servlet-name>
        <servlet-class>fr.laerce.todo.Todo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>todoft</servlet-name>
        <servlet-class>fr.laerce.todo.TodoFT</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>nagios</servlet-name>
        <servlet-class>nagios.NagiosCfg</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>nagiosLogList</servlet-name>
        <servlet-class>nagios.NagiosCfgLog</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>nagiosLogList</servlet-name>
        <url-pattern>/nagios-log</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>nagios</servlet-name>
        <url-pattern>/nagios</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>todoft</servlet-name>
        <url-pattern>/todoft/affiche</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>todoft</servlet-name>
        <url-pattern>/todoft/ajouter</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>todo</servlet-name>
        <url-pattern>/todo/affiche</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>todo</servlet-name>
        <url-pattern>/todo/ajoute</url-pattern>
    </servlet-mapping>

</web-app>
